			"item": [
				{
					"name": "Suche Film mit ID 1",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test('Statuscode 200', () => {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"pm.test('Content-Type application/json', () => {\r",
									"    const { response } = pm;\r",
									"    const contentType = response.headers.get('Content-Type');\r",
									"    pm.expect(contentType).to.match(/^application\\/json(; charset=utf-8)?/u);\r",
									"});\r",
									"pm.test('Body mit den Properties', () => {\r",
									"    const body = pm.response.json();\r",
									"    const { film } = body.data;\r",
									"    const { version, bewertung, genre, preis, datum, titel } = film;\r",
									"    pm.expect(version).to.be.a('number').that.is.gte(0);\r",
									"    pm.expect(bewertung).to.be.a('number').that.is.gte(0);\r",
									"    pm.expect(genre).to.be.a('string');\r",
									"    pm.expect(preis).to.be.a('number').that.is.gte(0);\r",
									"    pm.expect(datum).to.be.a('string');\r",
									"    pm.expect(titel.titel).to.be.a('string');\r",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "",
								"value": "",
								"type": "text",
								"disabled": true
							}
						],
						"body": {
							"mode": "graphql",
							"graphql": {
								"query": "query ($id: ID! = \"1\") {\r\n  film(id: $id) {\r\n     version\r\n    bewertung\r\n    genre\r\n    preis\r\n      datum\r\n         titel {\r\n      titel\r\n    }\r\n    }\r\n}",
								"variables": "{\r\n  \"id\": \"1\"\r\n}"
							}
						},
						"url": {
							"raw": "{{graphql_url}}",
							"host": [
								"{{graphql_url}}"
							]
						}
					},
					"response": []
				},
				
